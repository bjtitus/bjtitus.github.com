---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Brandon Titus</title>
  <link href="http://bjtitus.net/atom.xml" rel="self"/>
  <link href="http://bjtitus.net/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://bjtitus.net/</id>
  <author>
    <name>Brandon TItus</name>
    <email>me@bjtitus.net</email>
  </author>
  {% for post in site.posts limit:10 %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://rsms.me{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://bjtitus.net{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
    <author>
      <name>Brandon Titus</name>
      <uri>http://bjtitus.net/about/</uri>
    </author>
  </entry>
  {% endfor %}
</feed>